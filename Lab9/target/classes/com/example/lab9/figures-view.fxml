    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <children>
        <TableView id="tableSquares" fx:id="tableSquares" editable="true" prefHeight="241.0" prefWidth="351.0">
            <columns>
                <TableColumn id="side" fx:id="side" prefWidth="75.0" text="Side"/>
                <TableColumn id="perimeter" fx:id="perimeter" prefWidth="75.0" text="Perimeter"/>
                <TableColumn id="area" fx:id="area" prefWidth="75.0" text="Area"/>
